.planet, .star, .orbit-path {
  position: absolute;
  border-radius: 50%;
  width: var(--size);
  height: var(--size);
  top: calc(50% - var(--size) / 2);
  left: calc(50% - var(--size) / 2);
  box-shadow: -5px 0 25px rgba(255, 255, 255, 0.192);
}

